<Window x:Class="PollProgram.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:PollProgram"
        xmlns:pages="clr-namespace:PollProgram.Views.Pages"
        mc:Ignorable="d"
        Title="MainWindow" Height="768" Width="1280"
        WindowStartupLocation="CenterScreen">
    <Window.Resources>
        <pages:StartPage x:Key="StartPage"/>
    </Window.Resources>
    <Frame Name="MainFrame" NavigationUIVisibility="Hidden" Content="{StaticResource StartPage}"/>
</Window>
